    <script>
   function showTooltip(element, text) {
  const tooltipContainer = document.getElementById("tooltipContainer");
  tooltipContainer.innerHTML = text.replace(/\n/g, "<br>");
  tooltipContainer.style.top = `${element.offsetTop}px`;
  tooltipContainer.style.left = `${element.offsetLeft + element.offsetWidth}px`; // Adjusted to position the tooltip on the right side
  tooltipContainer.style.display = "block";
  tooltipContainer.style.color = "#FFFFFF";
  tooltipContainer.style.transform = "translateY(-50%)";
  tooltipContainer.style.transform = "translateX(10%)";


  const tooltipArrow = document.createElement("div");
  tooltipArrow.classList.add("tooltipArrow");
  tooltipContainer.appendChild(tooltipArrow);

  const tooltipArrowWidth = 10;
  const tooltipContainerWidth = tooltipContainer.offsetWidth;
  const tooltipArrowLeft = tooltipContainerWidth + tooltipArrowWidth;
  tooltipArrow.style.left = `${tooltipArrowLeft}px`;
}

document.addEventListener("DOMContentLoaded", function() {
  const tooltipContainer = document.getElementById("tooltipContainer");
  document.addEventListener("mouseout", function(event) {
    if (!event.relatedTarget || !event.relatedTarget.classList.contains("tooltip")) {
      tooltipContainer.style.display = "none";
    }
  });
});

    </script>      
        
